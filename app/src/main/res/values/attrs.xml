<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HierarchyLayout3">
        <attr name="hl_horizontalSpacing" format="dimension"/>
        <attr name="hl_verticalSpacing" format="dimension"/>
        <attr name="hl_connectLineColor" format="color"/>
        <attr name="hl_connectLineStrokeWidth" format="dimension"/>
        <attr name="hl_connectLineCornerPathEffect" format="dimension"/>
        <attr name="hl_connectLineEffectSize" format="dimension"/>
        <attr name="hl_setHasFixedSize" format="boolean"/>
        <attr name="hl_previewWidth" format="dimension"/>
        <attr name="hl_previewHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="HierarchyLayout4">
        <attr name="hl4_horizontalSpacing" format="dimension"/>
        <attr name="hl4_verticalSpacing" format="dimension"/>
        <attr name="hl4_connectLineColor" format="color"/>
        <attr name="hl4_connectLineStrokeWidth" format="dimension"/>
        <attr name="hl4_connectLineCornerPathEffect" format="dimension"/>
        <attr name="hl4_connectLineEffectSize" format="dimension"/>
        <attr name="hl4_hierarchyMinScale" format="float"/>
        <attr name="hl4_hierarchyMaxScale" format="float"/>
        <attr name="hl4_setHasFixedSize" format="boolean"/>
        <attr name="hl4_previewWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SeatTable1">
        <attr name="st_previewWidth" format="dimension"/>
        <attr name="st_hierarchyMinScale" format="float"/>
        <attr name="st_hierarchyMaxScale" format="float"/>
        <attr name="st_setHasFixedSize" format="boolean"/>
        <attr name="st_thumbBackgroundDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WaterFallLayout1">
        <attr name="wl_columnCount" format="integer"/>
        <attr name="wl_horizontalSpacing" format="dimension"/>
        <attr name="wl_verticalSpacing" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="BaoGridLayout1">
        <attr name="numColumns" format="integer" />
        <attr name="itemMargin" format="integer" />
    </declare-styleable>

    <declare-styleable name="HuiFlowLayout1">
        <attr name="item_padding" format="dimension"/>
        <attr name="item_divide_horizontal" format="dimension"/>
        <attr name="item_divide_vertical" format="dimension"/>
        <attr name="orientation" format="integer">
            <enum name="vertical" value="-1"/>
            <enum name="horizontal" value="-2"/>
        </attr>
        <attr name="item_padding_fixable" format="boolean"/>
        <attr name="columns" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TaoFlowLayoutView">
        <attr name="fl_columnsCount" format="integer|reference"/>
        <attr name="fl_horizontalSpacing" format="dimension|reference"/>
        <attr name="fl_verticalSpacing" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="LiangFunnyWaterFlowLayout1">
        <attr name="fwfl_columnNum" format="integer"/>
    </declare-styleable>

</resources>